<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.sunrun.washer.entity">
    <class name="TestPush" table="washer_test_push">
         <id name="testPushId" type="java.lang.Integer">
            <column name="test_push_id" />
            <generator class="identity" />
         </id>
         <property name="machineNo" column="machine_no" type="java.lang.String" not-null="true" length="255"/>
         <property name="registrationId" column="registration_id" type="java.lang.String" not-null="true" length="255"/>

    </class>
</hibernate-mapping>

